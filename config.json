{
    "baseURL": "http://adityakadoo.github.io/",
    "languageCode": "en-us",
    "title": "Scrolls",
    "theme": "Scrolls",
    "params": {
       "logos": {
          "icon": ":fish_cake:",
          "top": ":top:",
          "new": ":new:",
          "article": ":scroll:"
       },
       "custom_css": [
          "css/custom.css",
          "css/markdown.css",
          "css/syntax.css"
       ],
       "custom_js": [
          "js/custom.js"
       ]
    },
    "markup": {
       "goldmark": {
          "extensions": {
             "definitionList": true,
             "footnote": true,
             "linkify": true,
             "linkifyProtocol": "https",
             "strikethrough": true,
             "table": true,
             "taskList": true,
             "typographer": true
          },
          "parser": {
             "attribute": {
                "block": false,
                "title": true
             },
             "autoHeadingID": true,
             "autoHeadingIDType": "github",
             "wrapStandAloneImageWithinParagraph": true
          },
          "renderer": {
             "hardWraps": false,
             "unsafe": false,
             "xhtml": false
          }
       }
    },
    "enableRobotsTXT": true
 }